    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.12.19",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Delight\\Box\\": "src/"
        }
